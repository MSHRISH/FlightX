          "description": "A json object containing user_name,email,phone number,dob and password.",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/userDetails"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully a user is created.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/operationSuccess"
                }
              }
            }
          },
          "400": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/errorReport"
                }
              }
            }
          }
        }
      }
